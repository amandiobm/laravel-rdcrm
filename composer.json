{
    "name": "thiagovictorino/laravel-rdcrm",
    "description": "This is a non-official package for Laravel to integrate with RDS-CRM (plugcrm)",
    "keywords": [
        "thiagovictorino",
        "laravel-rdcrm"
    ],
    "homepage": "https://github.com/thiagovictorino/laravel-rdcrm",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Thiago Victorino",
            "email": "thiagovictorino@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Thiagovictorino\\LaravelRdcrm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thiagovictorino\\LaravelRdcrm\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thiagovictorino\\LaravelRdcrm\\LaravelRdcrmServiceProvider"
            ],
            "aliases": {
                "LaravelRdcrm": "Thiagovictorino\\LaravelRdcrm\\LaravelRdcrmFacade"
            }
        }
    }
}
